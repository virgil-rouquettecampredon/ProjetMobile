<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="390dp"
    android:height="390dp"
    android:viewportWidth="390"
    android:viewportHeight="390">
  <group>
    <clip-path
        android:pathData="m0,0l390,0l0,390l-390,0l0,-390z"/>

    <path
        android:pathData="m0.0,-0.008l390,0l0,390l-390,0z"
        android:strokeLineJoin="round"
        android:strokeWidth="100"
        android:fillColor="#00000000"
        android:fillType="evenOdd"
        android:strokeColor="#000000"
        android:strokeLineCap="butt"/>

    <path
        android:pathData="m0.0,-0.008l390,0l0,390l-390,0z"
        android:fillType="evenOdd"
        android:strokeColor="#00000000"
        android:strokeLineCap="square">
      <aapt:attr name="android:fillColor">
        <gradient
            android:gradientRadius="250"
            android:centerX="195"
            android:centerY="195"
            android:type="radial"
            android:tileMode="clamp">
          <item android:offset="0" android:color="#00000000"/>
          <item android:offset="0.8" android:color="#55000000"/>
          <item android:offset="1" android:color="#FF000000"/>
        </gradient>
      </aapt:attr>
    </path>
  </group>
</vector>
